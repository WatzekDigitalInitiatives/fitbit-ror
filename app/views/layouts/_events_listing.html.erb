<tr>
  <td><%= event.name %></td>
  <td><%= event.start_date %></td>
  <td><%= event.finish_date %></td>
  <td><%= event.start_location %></td>
  <td><%= event.end_location %></td>
  <td><%= event.invitecode %></td>
  <% if event.team_event %>
    <td><i class="material-icons">supervisor_account</i></td>
  <% else %>
    <td><i class="material-icons">perm_identity</i></td>
  <% end %>
  <% if event.private%>
    <td><i class="material-icons">lock</i></td>
  <% else %>
    <td><i class="material-icons">lock_open</i></td>
  <% end %>
  <td><%= link_to 'Show', event %></td>
  <% if current_user.present? && current_user.id == event.createdby %>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  <% end %>
</tr>
